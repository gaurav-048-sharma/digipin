{
  "version": 2,
  "functions": {
    "api/server.js": {
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "env": {
    "NODE_ENV": "production"
  },
  "rewrites": [
    {
      "source": "/api/digipin/(.*)",
      "destination": "/api/server.js"
    },
    {
      "source": "/api-docs",
      "destination": "/api/server.js"
    },
    {
      "source": "/(.*)",
      "destination": "/api/server.js"
    }
  ]
}
